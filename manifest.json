{
    "manifest_version": 2,
    "name": "Son Depremler (TR)",
    "description": "Türkiye'de son depremler hakkında bilgi",
    "version": "0.0.3",
    "icons": {
        "512": "assets/_images/icon_512.png"
    },
    "browser_action": {
        "default_icon": "assets/_images/icon_512.png",
        "default_popup": "eq.html"
    },
    "content_scripts": [
        {
            "js": [
                "assets/js/jquery-3.4.1.min.js",
                "assets/js/spinner.js",
                "eq.js"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ]
}
